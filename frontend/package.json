{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon",
    "build:analyze": "ANALYZE=true next build && tsc --project tsconfig.server.json",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "next build && NODE_ENV=production ts-node --project tsconfig.server.json server.ts",
    "type-check": "tsc",
    "build:ant-css": "lessc --js static/styles/ant-overrides.less static/styles/ant.css",
    "build:tailwind-css": "tailwind build static/styles/tailwind.css -o static/styles/tailwind.css -c tailwind.config.js"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^9.0.5",
    "@types/compression": "^1.0.1",
    "@zeit/next-css": "^1.0.1",
    "antd": "^3.23.2",
    "apollo-boost": "^0.4.4",
    "apollo-link-ws": "^1.0.19",
    "compression": "^1.7.4",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "next": "^9.0.5",
    "next-cookies": "^1.1.3",
    "next-images": "^1.1.2",
    "next-offline": "^4.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-apollo": "^3.1.0",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.9.0",
    "react-scrollbars-custom": "^4.0.20",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "author": "nirmalya.email@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/next": "^8.0.6",
    "@types/react": "^16.9.2",
    "@types/react-dom": "16.9.0",
    "cross-env": "^5.2.1",
    "husky": "^3.0.5",
    "lessc": "^1.0.2",
    "lint-staged": "^9.2.5",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "tailwindcss": "^1.1.2",
    "ts-node": "^8.3.0",
    "typescript": "3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
